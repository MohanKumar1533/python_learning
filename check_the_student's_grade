score = int(input("enter your score: "))
if 90< score<= 100:
	print("you got  'A' grade")
elif 80 < score <= 90:
	print("you got 'B' grade")
elif 60 < score <= 80:
	print("you got 'C' grade")
elif 35< score <= 60:
	print("you got 'D' grade")
elif 0 < score <= 35:
	print("you got 'F' grade")
else:
	print("invalid score")
